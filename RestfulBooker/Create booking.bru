meta {
  name: Create booking
  type: http
  seq: 4
}

post {
  url: {{baseUrl}}/booking
  body: json
  auth: bearer
}

auth:bearer {
  token: Bearer {{token}}
}

body:json {
  {
      "firstname" : "Jim",
      "lastname" : "Brown",
      "totalprice" : 111,
      "depositpaid" : true,
      "bookingdates" : {
          "checkin" : "2022-01-01",
          "checkout" : "2022-01-10"
      },
      "additionalneeds" : "Breakfast"
  }
}

assert {
  res.status: eq 200
}
